LOAD REG1 2
LOAD REG2 3
LOAD REG0 0
STORE REG0 8
loop:
LOAD REG0 REG1
LOAD REG3 REG2
JNZ [loop_sum]
INC REG1
INC REG2
LOAD REG0 8
DEC REG0
STORE REG0 8
JNZ [loop]
LOAD REG0 0
JNZ [end]
loop_sum:
INC REG0
DEC REG3
JNZ [loop_sum]
STORE REG0 REG1
INC REG1
INC REG2
LOAD REG0 8
DEC REG0
STORE REG0 8
JNZ [loop]
end:
INC REG1